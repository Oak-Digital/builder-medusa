{
  "name": "@oak-digital/builder-plugin-medusa",
  "version": "0.0.0",
  "type": "module",
  "unpkg": "dist/index.cjs",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "run-p dev:*",
    "dev:watch": "vite build --watch --mode production",
    "dev:serve": "serve --cors -l tcp://0.0.0.0:1268 dist",
    "build": "tsc && vite build",
    "build:rollup": "tsc --module commonjs && rollup -c rollup.config.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@builder.io/commerce-plugin-tools": "^0.3.0",
    "@builder.io/data-plugin-tools": "^0.0.4",
